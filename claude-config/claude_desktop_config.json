      "args": [
        "run",
        "-i",
        "--rm",
        "-v",
        "c:/Users/Elinzar/Documents/Tecnologo/Projecto2025/FinalProjectADF/KB-MCP:/app/config",
        "mcp/structured-output"
      ],
      "env": {}
    }
  }
