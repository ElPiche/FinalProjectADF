      "command": "docker",
      "args": [
        "run",
        "-i",
        "--rm",
        "-e",
        "ES_URL",
        "mcp/elasticsearch",
        "stdio"
      ],
      "env": {
        "ES_URL": "http://host.docker.internal:9200"
      }
    },
    "structured-output": {
